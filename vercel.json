{
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        },
        {
            "src": "./public/index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/events",
            "dest": "./index.js"
        },
        {
          "src": "/events/:name",
          "dest": "./index.js"
        },
        {
          "src": "/",
          "dest": "./index.js"
        }
    ]

}